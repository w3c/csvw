@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

 [
    a csvw:TableGroup;
    csvw:table [
      a csvw:Table;
      rdfs:label "string format (valid combinations)";
      rdfs:comment "If the datatype base is not numeric, boolean, a date/time type, or a duration type, the datatype format annotation provides a regular expression for the string values";
      csvw:row [
        a csvw:Row;
        csvw:describes [
          <test152.csv#NMTOKEN> "foobar"^^xsd:NMTOKEN;
          <test152.csv#anyUri> "http://example.org"^^xsd:anyURI;
          <test152.csv#normalizedString> "bar"^^xsd:normalizedString;
          <test152.csv#string1> "AaAaA";
          <test152.csv#string2> "GED";
          <test152.csv#string3> "foo bar"
        ];
        csvw:rownum 1;
        csvw:url <test152.csv#row=2>
      ];
      csvw:url <test152.csv>
    ]
 ] .
